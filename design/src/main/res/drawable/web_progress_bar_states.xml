<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <gradient
                android:centerColor="#eef4f2"
                android:endColor="#eef4f2"
                android:startColor="#eef4f2" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient
                    android:centerColor="#eca94b"
                    android:endColor="#eca94b"
                    android:startColor="#eca94b" />
            </shape>
        </clip>
    </item>
</layer-list>