<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="table_row">
        <attr name="primary_title" format="string" />
        <attr name="secondary_title" format="string" />
        <attr name="left_icon" format="reference" />
        <attr name="right_icon" format="reference" />
        <attr name="is_show_left_icon" format="boolean" />
        <attr name="is_show_right_icon" format="boolean" />
        <attr name="primary_title_size" format="dimension" />
        <attr name="primary_title_color" format="color" />
        <attr name="secondary_title_size" format="dimension" />
        <attr name="secondary_title_color" format="color" />
    </declare-styleable>

    <declare-styleable name="search_edit_text">
        <attr name="clear_icon" format="reference" />
        <attr name="icon" format="reference" />
    </declare-styleable>

    <declare-styleable name="UnderlinedTextView">
        <attr name="underlineWidth" format="dimension" />
        <attr name="underlineColor" format="color" />
    </declare-styleable>

    <declare-styleable name="primary_toolbar">
        <attr name="tb_title" format="reference" />
        <attr name="tb_left_icon" format="reference" />
        <attr name="tb_right_icon" format="reference" />
        <attr name="tb_left_text" format="reference" />
        <attr name="tb_right_text" format="reference" />
    </declare-styleable>

</resources>